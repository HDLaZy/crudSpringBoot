<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lk.testlayui.mapper.EmpMapper">

    <resultMap id="BaseResultMap" type="com.lk.testlayui.pojo.Emp">
            <id property="empId" column="emp_id" jdbcType="INTEGER"/>
            <result property="empName" column="emp_name" jdbcType="VARCHAR"/>
            <result property="empDept" column="emp_dept" jdbcType="INTEGER"/>
            <result property="empSalary" column="emp_salary" jdbcType="DECIMAL"/>
            <result property="empSpace" column="emp_space" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        emp_id,emp_name,emp_dept,
        emp_salary,emp_space
    </sql>

</mapper>
